includes:

parameters:
    level: 8
    paths:
        - src
        - tests
    tmpDir: var/phpstan
    checkGenericClassInNonGenericObjectType: false
    reportUnmatchedIgnoredErrors: true
    scanFiles:
        - vendor/autoload.php
        - tools/coverage-guard.php
    stubFiles:
        - stubs/phpunit-mock-object.stub.php
